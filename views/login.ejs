	<script >
		$('.js-tilt').tilt({
			scale: 1.1
		})
	</script>

	<script src="javascripts/main1.js"></script>
	<script src="javascripts/jquery.min.js"></script>
  <script src="javascripts/jquery-migrate-3.0.1.min.js"></script>
